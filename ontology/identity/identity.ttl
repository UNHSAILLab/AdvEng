
@prefix engage: <https://ontology.adversaryengagement.org/ae/engage/> . 
@prefix identity: <https://ontology.adversaryengagement.org/ae/identity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> . 
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


identity:approachIdentifier a owl:DatatypeProperty ;
    rdfs:label "approachIdentifier"@en ;
    rdfs:comment "The unique identifier of the MITRE ENGAGE approach."@en ;
    rdfs:range xsd:string .

identity:goalIdentifier a owl:DatatypeProperty ;
    rdfs:label "goalIdentifier"@en ;
    rdfs:comment "The unique identifier of the MITRE ENGAGE goal."@en ;
    rdfs:range xsd:string .

 

engage:Persona a owl:Class, 
        sh:NodeShape ; 
    rdfs:label "Persona"@en ; 
    rdfs:comment " An persona is a facticious entity created to serve a purpose in a deception operation."@en ; 
    rdfs:subClassOf uco-identity:Identity ;
    sh:targetClass engage:Persona . 

identity:tacticIdentifier a owl:DatatypeProperty ;
    rdfs:label "tacticIdentifier"@en ;
    rdfs:comment "The unique identifier of the MITRE ATT&CK tactic."@en ;
    rdfs:range xsd:string .

identity:teamIdentifier a owl:DatatypeProperty ;
    rdfs:label "teamIdentifier"@en ;
    rdfs:comment "The unique identifier of a team."@en ;
    rdfs:range xsd:string .

identity:techniqueIdentifier a owl:DatatypeProperty ;
    rdfs:label "techniqueIdentifier"@en ;
    rdfs:comment "The unique identifier of the MITRE ATT&CK technique."@en ;
    rdfs:range xsd:string .