
@prefix engage: <https://ontology.adversaryengagement.org/ae/engage/> .
@prefix identity: <https://ontology.adversaryengagement.org/ae/identity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


engage:honeyTokenIdentifier a owl:DatatypeProperty ;
    rdfs:label "honeyTokenIdentifier"@en ;
    rdfs:comment "The unique identifier for a honey token."@en ;
    rdfs:range xsd:string .


identity:persona a owl:Class,
        sh:NodeShape ;
    rdfs:label "persona"@en ;
    rdfs:comment "An persona is a fabricated or partially ficticious person that serves some purpose for adversary deception."@en ;
    rdfs:subClassOf uco-identity:Person ;
    sh:targetClass identity:persona .

engage:TeamIdentifier a owl:DatatypeProperty ;
    rdfs:label "teamIdentifier"@en ;
    rdfs:comment "The unique identifier for the team."@en ;
    rdfs:range xsd:string .

engage:teamMemberIdentifier a owl:DatatypeProperty ;
    rdfs:label "teamMemberIdentifier"@en ;
    rdfs:comment "The unique identifier for a team member."@en ;
    rdfs:range xsd:string .

identity:teamName a owl:DatatypeProperty ;
    rdfs:label "teamName"@en ;
    rdfs:comment ""@en ;
    rdfs:range xsd:string .