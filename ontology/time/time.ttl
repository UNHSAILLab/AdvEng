
@prefix owl: <http://www.w3.org/2002/07/owl#> . 
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> . 
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> . 
@prefix sh: <http://www.w3.org/ns/shacl#> . 
@prefix time: <https://ontology.adversaryengagement.org/ae/time/> . 
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> . 
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-observable: <https://ontology.unifiedcyberontology.org/uco/observable/> . 
@prefix uco-vocabulary: <https://ontology.unifiedcyberontology.org/uco/vocabulary/> . 
@prefix vocabulary: <https://ontology.adversaryengagement.org/ae/vocabulary/> . 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> . 


 
time:hasTime a owl:ObjectProperty ;
    rdfs:label "hasTime"@en ; 
    rdfs:comment "An object that has a time object / property is in reference to a metric or measure and a contextual type."@en ; 
    rdfs:range time:Time .

 
time:Time a owl:Class, 
        sh:NodeShape ; 
    rdfs:label "Time"@en ; 
    rdfs:comment "Time is in reference to a metric or measure and a contextual type."@en ; 
    rdfs:subClassOf uco-core:UcoObject ; 
    sh:property [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path time:Unit ],
        [ sh:datatype xsd:dateTime ; 
            sh:maxCount 1 ; 
            sh:nodeKind sh:Literal ; 
            sh:path uco-observable:observableCreatedTime ], 
        [ sh:datatype xsd:dateTime ; 
            sh:maxCount 1 ; 
            sh:nodeKind sh:Literal ; 
            sh:path time:Timestamp ], 
        [ sh:datatype vocabulary:TimeTypeVocab ;
            sh:message "Value is outside the default vocabulary TimeTypeVocab." ;
            sh:path time:TimeType ;
            sh:severity sh:Info ],
        [ sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:or ( [ sh:datatype vocabulary:TimeTypeVocab ] [ sh:datatype xsd:string ] ) ;
            sh:path time:TimeType ],
		[ sh:message "Value is not member of the vocabulary TimeTypeVocab." ;
			sh:or ( [ sh:datatype vocabulary:TimeTypeVocab ;
						sh:in (
							"deploy"^^vocabulary:TimeTypeVocab
							"termination"^^vocabulary:TimeTypeVocab
							"duration"^^vocabulary:TimeTypeVocab
							) ;
					]
				[ sh:datatype xsd:string ; ] ) ;
			sh:path time:TimeType ; ] ;
    sh:targetClass time:Time ; . 

 
time:Timestamp a owl:DatatypeProperty ;
    rdfs:label "Timestamp"@en ;
    rdfs:comment "A timestamp is an instance of dateTime data structure representing some event along the time domain."@en ;
    rdfs:range xsd:dateTime .

 
time:TimeType a owl:DatatypeProperty ;
    rdfs:label "TimeType"@en ;
    rdfs:comment "The type of time."@en ;
    rdfs:range xsd:string .

 
time:Unit a owl:Class, 
        sh:NodeShape ; 
    rdfs:label "Unit"@en ; 
    rdfs:comment "A time unit is a time measure or metric e.g. hours, minutes, etc."@en ; 
    rdfs:subClassOf uco-core:UcoObject ; 
    sh:property [ sh:datatype vocabulary:UnitTypeVocab ;
            sh:message "Value is outside the default vocabulary UnitTypeVocab." ;
            sh:path time:UnitType ;
            sh:severity sh:Info ],
        [ sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:or ( [ sh:datatype vocabulary:UnitTypeVocab ] [ sh:datatype vocabulary:TimestampPercisionVocab ] [ sh:datatype xsd:string ] ) ;
            sh:path time:UnitType ],
		[ sh:message "Value is not member of the vocabulary UnitTypeVocab or TimestampPercisionVocab." ;
			sh:or ( [ sh:datatype vocabulary:UnitTypeVocab ;
						sh:in (
							"s"^^vocabulary:UnitTypeVocab
							"m"^^vocabulary:UnitTypeVocab
							"h"^^vocabulary:UnitTypeVocab
							"D"^^vocabulary:UnitTypeVocab
							"W"^^vocabulary:UnitTypeVocab
							"M"^^vocabulary:UnitTypeVocab
							"Y"^^vocabulary:UnitTypeVocab
							"Undefined"^^vocabulary:UnitTypeVocab 
							) ;
					]
					[ sh:datatype uco-vocabulary:TimestampPercisionVocab ;
							sh:in (
								"day"^^uco-vocabulary:TimestampPercisionVocab
								"minute"^^uco-vocabulary:TimestampPercisionVocab
								"hour"^^uco-vocabulary:TimestampPercisionVocab
								"month"^^uco-vocabulary:TimestampPercisionVocab
								"second"^^uco-vocabulary:TimestampPercisionVocab
								"year"^^uco-vocabulary:TimestampPercisionVocab
								) ;
						]
				[ sh:datatype xsd:string ; ] ) ;
			sh:path time:UnitType ; ] ;
    sh:targetClass time:Unit . 

 
time:UnitType a owl:DatatypeProperty ;
    rdfs:label "UnitType"@en ;
    rdfs:comment "The type of time unit."@en ;
    rdfs:range xsd:string .